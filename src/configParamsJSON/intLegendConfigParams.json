                      ]
                    },
                    {
                      "type": "setting",
                      "id": "enablePopupOption",
                      "defaultValue": false,
                      "content": [
                        {
                          "type": "setting",
                          "id": "includePopupInScreenshot",
                          "defaultValue": false
