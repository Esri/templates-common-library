                  "type": "setting",
                  "id": "enableHighlightHaloColor",
                  "express": false,
                  "content": [
                    {
                      "type": "setting",
                      "id": "highlightHaloColor",
                      "express": false,
                      "defaultValue": "#00FFFF"
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "subsection",
          "id": "share",
          "content": [
            {
              "type": "group",
              "id": "share",
              "content": [
                {
                  "type": "setting",
                  "id": "share",
                  "defaultValue": false,
                  "express": true,
                  "content": [
                    {
                      "type": "setting",
                      "id": "shareIncludeSocial",
                      "defaultValue": true,
                      "express": false
