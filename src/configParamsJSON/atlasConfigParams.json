{
  "config": [
    {
      "type": "section",
      "id": "atlasGroup",
      "content": [
        {
          "type": "setting",
          "id": "group",
          "defaultValue": "",
          "express": true
        }
      ]
    },
    {
      "type": "section",
      "id": "atlas",
      "content": [
        {
          "type": "setting",
          "id": "home",
          "express": true,
          "defaultValue": true
        },
        {
          "type": "setting",
          "id": "mapZoom",
          "express": true,
          "defaultValue": true
        },
        {
          "type": "setting",
          "id": "basemapSelector",
          "express": true,
          "defaultValue": "streets-vector"
        }
      ]
    },
    {
      "type": "section",
      "id": "about",
      "content": [
        {
          "type": "setting",
          "id": "header",
          "defaultValue": true,
          "express": true,
          "content": [
            {
              "type": "setting",
              "id": "title",
              "defaultValue": "Atlas"
            }
          ]
        },
        {
          "type": "setting",
          "id": "splash",
          "express": true,
          "defaultValue": false,
          "content": [
            {
              "type": "setting",
              "id": "splashTitle",
              "defaultValue": "Welcome to Atlas"
            },
            {
              "type": "setting",
              "id": "splashContent",
              "defaultValue": "Explore available geographic content. Click Details to learn more about the data. Click View to add data to the map."
            },
            {
              "type": "setting",
              "id": "splashButtonText",
              "defaultValue": "Get Started"
            }

          ]
        }
      ]
    },
    {
      "type": "section",
      "id": "interactivity",
      "content": [
        {
          "type": "setting",
          "id": "search",
          "express": true,
          "defaultValue": true,
          "content": [

            {
              "type": "setting",
              "id": "searchOpenAtStart",
              "express": true,
              "defaultValue": false
            },
            {
              "type": "setting",
              "id": "searchConfiguration",
              "express": true,
              "config": {
                "includeDefaultLocators": false
              },
              "defaultValue": null
            }
          ]
        }
      ]
    },
    {
      "type": "section",
      "id": "themeLayout",
      "content": [
        {
          "type": "subsection",
          "id": "theme",
          "content": [
            {
              "type": "setting",
              "id": "theme",
              "defaultValue": "light",
              "express": true
            },
            {
              "type": "group",
              "id": "headerTheme",
              "content": [
                {
                  "type": "setting",
                  "id": "customTheme",
                  "express": true,
                  "config": {
                    "numOfSections": 1,
                    "headerOnly": true,
                    "singleFont": true
                  },
                  "defaultValue": null
                }
              ]
            }
          ]
        },
        {
          "type": "subsection",
          "id": "logo",
          "content": [
            {
              "type": "group",
              "id": "logo",
              "content": [
                {
                  "type": "setting",
                  "id": "logo",
                  "express": false,
                  "defaultValue": null
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}
