{
  "config": [
    {
      "type": "section",
      "id": "map"
    },
    {
      "type": "section",
      "id": "insets",
      "content": [
        {
          "type": "group",
          "id": "insets",
          "content": [
            {
              "type": "setting",
              "id": "insetsConfig",
              "express": true,
              "defaultValue": null
            }
          ]
        }
      ]
    },
    {
      "type": "section",
      "id": "about",
      "content": [
        {
          "type": "subsection",
          "id": "appComprehension",
          "content": [
            {
              "type": "group",
              "id": "appComprehension",
              "content": [
                {
                  "type": "setting",
                  "id": "title",
                  "defaultValue": "",
                  "express": true
                },
                {
                  "type": "setting",
                  "id": "titleLink",
                  "defaultValue": "",
                  "express": false
                },
                {
                  "type": "setting",
                  "id": "splash",
                  "defaultValue": false,
                  "express": true,
                  "content": [
                    {
                      "type": "setting",
                      "id": "splashOnStart",
                      "defaultValue": true,
                      "express": false
                    },
                    {
                      "type": "setting",
                      "id": "splashTitle",
                      "defaultValue": "Welcome!",
                      "express": false
                    },
                    {
                      "type": "setting",
                      "id": "splashContent",
                      "defaultValue": "Use forward and back arrows to navigate slides",
                      "express": true
                    },
                    {
                      "type": "setting",
                      "id": "splashButtonText",
                      "defaultValue": "Explore",
                      "express": false
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "subsection",
          "id": "coverPageSettings",
          "content": [
            {
              "type": "group",
              "id": "coverPage",
              "content": [
                {
                  "type": "setting",
                  "id": "coverPage",
                  "express": false,
                  "defaultValue": false,
                  "content": [
                    {
                      "type": "setting",
                      "id": "coverPageConfig",
                      "express": false,
                      "defaultValue": {
                        "title": "Inset",
                        "titleColor": "#ffffff",
                        "subtitle": "",
                        "subtitleColor": "#ffffff",
                        "textPosition": "center-leading",
                        "buttonText": "Explore",
                        "buttonTextColor": "#ffffff",
                        "background": {
                          "backgroundType": "color",
                          "backgroundImage": null,
                          "backgroundColor": "#0079c1"
                        }
                      }
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "subsection",
          "id": "featureComprehension",
          "content": [
            {
              "type": "group",
              "id": "featureComprehension",
              "content": [
                {
                  "type": "setting",
                  "id": "legend",
                  "defaultValue": false,
                  "content": [
                    {
                      "type": "setting",
                      "id": "legendPosition",
                      "defaultValue": {
                        "position": "bottom-left",
                        "index": 1
                      }
                    },
                    {
                      "type": "setting",
                      "id": "legendOpenAtStart",
                      "defaultValue": false
                    }
                  ]
                },
                {
                  "type": "setting",
                  "id": "popupFixedPosition",
                  "express": true,
                  "defaultValue": "top-right"
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "section",
      "id": "interactivity",
      "content": [
        {
          "type": "subsection",
          "id": "exploreNavigate",
          "content": [
            {
              "type": "group",
              "id": "explore",
              "content": [
                {
                  "type": "setting",
                  "id": "mapZoom",
                  "express": true,
                  "defaultValue": true,
                  "content": [
                    {
                      "type": "setting",
                      "id": "mapZoomPosition",
                      "defaultValue": {
                        "position": "top-left",
                        "index": 2
                      }
                    }
                  ]
                },
                {
                  "type": "setting",
                  "id": "home",
                  "express": true,
                  "defaultValue": true
                },
                {
                  "type": "setting",
                  "id": "locateWidget",
                  "express": true,
                  "defaultValue": false
                },
                {
                  "type": "setting",
                  "id": "extentSelector",
                  "defaultValue": false,
                  "content": [
                    {
                      "type": "setting",
                      "id": "extentSelectorConfig",
                      "defaultValue": null
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "subsection",
          "id": "modify",
          "content": [
            {
              "type": "setting",
              "id": "layerList",
              "defaultValue": false,
              "content": [
                {
                  "type": "setting",
                  "id": "layerListPosition",
                  "defaultValue": {
                    "position": "bottom-right",
                    "index": 0
                  }
                },
                {
                  "type": "setting",
                  "id": "layerListOpenAtStart",
                  "express": false,
                  "defaultValue": false
                }
              ]
            }
          ]
        },
        {
          "type": "subsection",
          "id": "share",
          "content": [
            {
              "type": "group",
              "id": "share",
              "content": [
                {
                  "type": "setting",
                  "id": "exportToPDF",
                  "defaultValue": false,
                  "express": false
                },
                {
                  "type": "setting",
                  "id": "share",
                  "defaultValue": false,
                  "express": false
                }
              ]
            }
          ]
        },
        {
          "type": "subsection",
          "id": "search",
          "content": [
            {
              "type": "group",
              "id": "search",
              "content": [
                {
                  "type": "setting",
                  "id": "search",
                  "defaultValue": false,
                  "express": true,
                  "content": [
                    {
                      "type": "setting",
                      "id": "searchPosition",
                      "defaultValue": {
                        "position": "top-right",
                        "index": 1
                      }
                    },
                    {
                      "type": "setting",
                      "id": "searchOpenAtStart",
                      "defaultValue": false,
                      "express": false
                    },
                    {
                      "type": "setting",
                      "id": "searchConfiguration",
                      "express": true,
                      "defaultValue": null,
                      "config": {
                        "includeDefaultLocators": false
                      }
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "section",
      "id": "themeLayout",
      "content": [
        {
          "type": "subsection",
          "id": "theme",
          "content": [
            {
              "type": "group",
              "id": "theme",
              "content": [
                {
                  "type": "setting",
                  "id": "theme",
                  "express": true,
                  "defaultValue": "light"
                },
                {
                  "type": "setting",
                  "id": "customTheme",
                  "express": true,
                  "defaultValue": null,
                  "config": {
                    "numOfSections": 1,
                    "headerOnly": true,
                    "fontAlwaysEditable": true
                  }
                }
              ]
            }
          ]
        },
        {
          "type": "subsection",
          "id": "insetStyle",
          "content": [
            {
              "type": "setting",
              "id": "insetStyle",
              "defaultValue": {
                "style": "square",
                "outline": "1px",
                "dropShadow": true
              },
              "express": true
            }
          ]
        },
        {
          "type": "subsection",
          "id": "positionManager",
          "content": [
            {
              "type": "setting",
              "id": "positionManager",
              "express": true,
              "defaultValue": null
            },
            {
              "type": "setting",
              "id": "inset1Position",
              "defaultValue": {
                "position": "bottom-left",
                "index": 0
              }
            },
            {
              "type": "setting",
              "id": "inset2Position",
              "defaultValue": {
                "position": "bottom-right",
                "index": 0
              }
            },
            {
              "type": "setting",
              "id": "inset3Position",
              "defaultValue": {
                "position": "top-left",
                "index": 0
              }
            },
            {
              "type": "setting",
              "id": "inset4Position",
              "defaultValue": {
                "position": "top-right",
                "index": 0
              }
            }
          ]
        }
      ]
    }
  ]
}
