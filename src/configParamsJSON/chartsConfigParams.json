{
  "config": [
    {
      "type": "section",
      "id": "map",
      "config": {
        "conditions": [
          "charts"
        ]
      }
    },
    {
      "type": "section",
      "id": "charts",
      "content": [
        {
          "type": "group",
          "id": "charts",
          "content": [
            {
              "type": "setting",
              "id": "chartsLayout",
              "express": true,
              "config": {
                "branches": [
                  [
                    "panelSize"
                  ],
                  []
                ]
              },
              "content": [
                {
                  "type": "setting",
                  "id": "panelSize",
                  "express": true,
                  "defaultValue": "m"
                }
              ],
              "defaultValue": {
                "branchValue": "stack",
                "branchOptionsFieldNames": [
                  "panelSize"
                ]
              }
            },
            {
              "type": "setting",
              "id": "numChartsOnload",
              "express": false,
              "defaultValue": 1
            },
            {
              "type": "setting",
              "id": "chartsConfig",
              "express": true,
              "defaultValue": null
            }
          ]
        }
      ]
    },
    {
      "type": "section",
      "id": "about",
      "content": [
        {
          "type": "subsection",
          "id": "appComprehension",
          "content": [
            {
              "type": "group",
              "id": "appComprehension",
              "content": [
                {
                  "type": "setting",
                  "id": "header",
                  "defaultValue": true,
                  "express": true,
                  "content": [
                    {
                      "type": "setting",
                      "id": "title",
                      "defaultValue": "",
                      "express": true
                    }
                  ]
                },
                {
                  "type": "setting",
                  "id": "coverPage",
                  "express": true,
                  "defaultValue": false,
                  "content": [
                    {
                      "type": "setting",
                      "id": "coverPageConfig",
                      "express": true,
                      "defaultValue": {
                        "title": "Charts",
                        "titleColor": "#ffffff",
                        "subtitle": "View and analyze charts in your map!",
                        "subtitleColor": "#ffffff",
                        "textPosition": "center-leading",
                        "buttonText": "Explore",
                        "buttonTextColor": "#ffffff",
                        "background": {
                          "backgroundType": "color",
                          "backgroundImage": null,
                          "backgroundColor": "#0079c1"
                        }
                      }
                    }
                  ]
                },
                {
                  "type": "setting",
                  "id": "splash",
                  "defaultValue": false,
                  "express": true,
                  "content": [
                    {
                      "type": "setting",
                      "id": "splashButtonPosition",
                      "defaultValue": {
                        "position": "top-left",
                        "index": 2
                      }
                    },
                    {
                      "type": "setting",
                      "id": "splashOnStart",
                      "defaultValue": false,
                      "express": true
                    },
                    {
                      "type": "setting",
                      "id": "splashTitle",
                      "defaultValue": "Welcome!",
                      "express": true
                    },
                    {
                      "type": "setting",
                      "id": "splashContent",
                      "defaultValue": "View and analyze charts in your map!",
                      "express": true
                    },
                    {
                      "type": "setting",
                      "id": "splashButtonText",
                      "defaultValue": "Explore",
                      "express": true
                    }
                  ]
                }
              ]
            },
            {
              "type": "group",
              "id": "mapA11y",
              "content": [
                {
                  "type": "setting",
                  "id": "mapA11yDesc",
                  "express": true
                }
              ]
            }
          ]
        },
        {
          "type": "subsection",
          "id": "featureComprehension",
          "content": [
            {
              "type": "group",
              "id": "featureComprehension",
              "content": [
                {
                  "type": "setting",
                  "id": "legend",
                  "defaultValue": false,
                  "express": false,
                  "content": [
                    {
                      "type": "setting",
                      "id": "legendConfig",
                      "express": false,
                      "defaultValue": {
                        "style": {
                          "type": "classic",
                          "layout": "stack"
                        }
                      }
                    },
                    {
                      "type": "setting",
                      "id": "legendPosition",
                      "defaultValue": {
                        "position": "top-right",
                        "index": 0
                      }
                    },
                    {
                      "type": "setting",
                      "id": "legendOpenAtStart",
                      "defaultValue": false
                    }
                  ]
                },
                {
                  "type": "setting",
                  "id": "popupFixed",
                  "express": true,
                  "defaultValue": false,
                  "content": [
                    {
                      "type": "setting",
                      "id": "popupFixedPosition",
                      "express": true,
                      "defaultValue": "bottom-right"
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    },
