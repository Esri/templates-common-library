                    {
                      "type": "setting",
                      "id": "shareWithOrgPosition",
                      "express": true,
                      "defaultValue": "top-right"
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "section",
      "id": "themeLayout",
      "content": [
        {
          "type": "subsection",
          "id": "theme",
          "content": [
            {
              "type": "setting",
              "id": "theme",
              "express": true,
              "defaultValue": "light"
            },
            {
              "type": "setting",
              "id": "customTheme",
              "express": true,
              "defaultValue": null,
              "config": {
                "singleFont": false
